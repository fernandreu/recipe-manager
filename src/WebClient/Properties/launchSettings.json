{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56042/recipe-manager/",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "WebClient": {
      "commandName": "Project",
      "launchBrowser": true,
      "commandLineArgs": "--pathbase=/recipe-manager",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:50001"
    },
    "WebClientRider": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(_BlazorDevServerDll)\" serve --pathbase=/recipe-manager --applicationpath \"$(TargetPath)\"",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Kestrel:Endpoints:HTTP:Url": "http://localhost:50000",
        "Kestrel:Endpoints:HTTPS:Url": "https://localhost:50001"
      }
    }
  }
}